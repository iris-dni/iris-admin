(function (global) {
  global.iris = global.iris || {};
  settings = global.iris.settings = global.iris.settings || {};
  settings.swagger = {
    specUrl: '{{ "http://" }}{{ config['containers']['iris-api-service']['url'] }}/swagger.json'
  };
  settings.ssoProviders = [
    {%- for provider in config['containers']['iris-api-service']['ssoProviders'] %}
    {
      name: '{{ provider['name']}}',
      loginUrl: '{{ provider['loginUrl'] }}'
      }{%if not loop.last %},{% endif %}
    {%- endfor %}
  ];
}(window));
